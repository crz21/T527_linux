/dts-v1/;

#include "rk3576.dtsi"
#include "rk3576-evb1.dtsi"
#include "rk3576-linux.dtsi"

/ {
	model = "TL3576-MiniEVM";
	compatible = "tronlong,tl3576-minievm", "rockchip,rk3576";

	vbus5v0_typec: vbus5v0-typec {
		compatible = "regulator-fixed";
		regulator-name = "vbus5v0_typec";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		enable-active-high;
		gpio = <&gpio0 RK_PC4 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc5v0_device>;
	};

	wireless_bluetooth: wireless-bluetooth {
		compatible = "bluetooth-platdata";
		status = "disabled";
	};
	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";

		/*
		 * On the module itself this is one of these (depending
		 * on the actual card populated):
		 * - SDIO_RESET_L_WL_REG_ON
		 * - PDN (power down when low)
		 */
		post-power-on-delay-ms = <200>;
		status = "okay";
	};

	leds {
		status = "okay";
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&som_led0 &som_led1 &user_led0 &user_led1>;

		som_led0 {
			label = "heartbeat";
			gpios = <&gpio0 RK_PB6 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "heartbeat";
			default-state = "off";
		};

		som_led1 {
			label = "disk";
			gpios = <&gpio0 RK_PD2 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "mmc0";
			default-state = "off";
		};

		user_led0 {
			label = "user-led0";
			gpios = <&gpio0 RK_PB5 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "default-on";
			default-state = "off";
		};

		user_led1 {
			label = "user-led1";
			gpios = <&gpio0 RK_PB4 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "default-on";
			default-state = "off";
		};
	};

	/delete-node/ adc-keys;

        adc-keys1 {
            compatible = "adc-keys";
            io-channels = <&saradc 0>;
            io-channel-names = "buttons";
            keyup-threshold-microvolt = <1800000>;
            poll-interval = <100>;

                user_key2 {
                    label = "user-key2";
                    linux,code = <KEY_PROG3>;
                    press-threshold-microvolt = <1750>;
                };
        };

        adc-keys2 {
            compatible = "adc-keys";
            io-channels = <&saradc 1>;
            io-channel-names = "buttons";
            keyup-threshold-microvolt = <1800000>;
            poll-interval = <100>;

                user_key0 {
                    label = "user-key0";
                    linux,code = <KEY_PROG1>;
                    press-threshold-microvolt = <1750>;
                };
        };

	export-gpio {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_pins_group1 &gpio_pins_group2>;
	};
};

&fiq_debugger {
	rockchip,baudrate = <115200>;
};

&cluster0_opp_table{
        /* RK3576 A53 core limit at 1.4GHz */
        /delete-node/ opp-1608000000;
        /delete-node/ opp-1800000000;
        /delete-node/ opp-2016000000;

        /* RK3576J A53 core limit at 1.4GHz */
        /delete-node/ opp-j-m-1608000000;
        /delete-node/ opp-j-m-1800000000;
        /delete-node/ opp-j-m-1920000000;
};

&cluster1_opp_table{
        /* RK3576 A72 core limit at 1.6GHz */
        /delete-node/ opp-1800000000;
        /delete-node/ opp-2016000000;
        /delete-node/ opp-2208000000;

        /* RK3576J A72 core limit at 1.6GHz */
        /delete-node/ opp-j-m-1800000000;
        /delete-node/ opp-j-m-2016000000;
        /delete-node/ opp-j-m-2112000000;
};

&gpu_opp_table {
	/* RK3576 GPU limit at 700MHz */
	/delete-node/ opp-s-800000000;
	/delete-node/ opp-800000000;
	/delete-node/ opp-900000000;

	/* RK3576J GPU limit at 700MHz */
	/delete-node/ opp-j-m-800000000;
};

&npu_opp_table {
	/* RK3576 NPU limit at 700MHz */
	/delete-node/ opp-800000000;
	/delete-node/ opp-900000000;
	/delete-node/ opp-950000000;

	/* RK3576J NPU limit at 700MHz */
	/delete-node/ opp-j-m-800000000;
	/delete-node/ opp-j-m-850000000;
};

&dmc {
	system-status-level = <
		/* system status        freq level */
		SYS_STATUS_NORMAL       DMC_FREQ_LEVEL_HIGH
	>;
	auto-freq-en = <0>;
	status = "disabled";
};

