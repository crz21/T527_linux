&gmac0 {
	/* Use rgmii-rxid mode to disable rx delay inside Soc */
	phy-mode = "rgmii-rxid";
	clock_in_out = "output";

	snps,reset-gpio = <&gpio3 RK_PA6 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;

	pinctrl-names = "default";
	pinctrl-0 = <&eth0m1_miim
		     &eth0m1_tx_bus2
		     &eth0m1_rx_bus2
		     &eth0m1_rgmii_clk
		     &eth0m1_rgmii_bus>;

	tx_delay = <0x21>;
	/* rx_delay = <0x3f>; */

	phy-handle = <&rgmii_phy0>;
	status = "okay";
};

&gmac1 {
	/* Use rgmii-rxid mode to disable rx delay inside Soc */
	phy-mode = "rgmii-rxid";
	clock_in_out = "output";

	snps,reset-gpio = <&gpio3 RK_PA7 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;

	pinctrl-names = "default";
	pinctrl-0 = <&eth1m0_miim
		     &eth1m0_tx_bus2
		     &eth1m0_rx_bus2
		     &eth1m0_rgmii_clk
		     &eth1m0_rgmii_bus>;

	tx_delay = <0x20>;
	/* rx_delay = <0x3f>; */

	phy-handle = <&rgmii_phy1>;
	status = "okay";
};

&mdio0 {
	rgmii_phy0: phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x0>;
		clocks = <&cru REFCLKO25M_GMAC0_OUT>;
	};
};

&mdio1 {
	rgmii_phy1: phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x1>;
		clocks = <&cru REFCLKO25M_GMAC1_OUT>;
	};
};

&pwm2_8ch_6 {
	status = "okay";
	pinctrl-0 = <&pwm2m2_ch6>;
};

&backlight {
        pwms = <&pwm2_8ch_6 0 25000 0>;
        status = "okay";
};

&usbdp_phy {
	orientation-switch;
	svid = <0xff01>;
	sbu1-dc-gpios = <&gpio4 RK_PC4 GPIO_ACTIVE_HIGH>;
	sbu2-dc-gpios = <&gpio4 RK_PC5 GPIO_ACTIVE_HIGH>;

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		usbdp_phy_orientation_switch: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usbc0_orien_sw>;
		};

		usbdp_phy_dp_altmode_mux: endpoint@1 {
			reg = <1>;
			remote-endpoint = <&dp_altmode_mux>;
		};
	};
};

&es8388_sound {
        /delete-property/ spk-con-gpio;
        hp-det-gpio = <&gpio4 RK_PB2 GPIO_ACTIVE_HIGH>;
        hp-con-gpio = <&gpio0 RK_PC3 GPIO_ACTIVE_HIGH>;
};

&pinctrl {
	leds {
		som_led0: som-led0 {
			rockchip,pins = <0 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		som_led1: som-led1 {
			rockchip,pins = <0 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		user_led0: user-led0 {
			rockchip,pins = <0 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		user_led1: user-led1 {
			rockchip,pins = <0 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb {
		usbc0_int: usbc0-int {
			rockchip,pins = <3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	touch {
		gt911_int: gt911-int {
			rockchip,pins = <4 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins = <4 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	export1 {
		gpio_pins_group1: gpio-pins-group1 {
			rockchip,pins =
			<1 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>,
			<4 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD0 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>,
			<2 RK_PD7 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD7 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	export2 {
		gpio_pins_group2: gpio-pins-group2 {
			rockchip,pins =
			<4 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>,
			<4 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>,
			<4 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>,
			<4 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>,
			<4 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD0 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD1 RK_FUNC_GPIO &pcfg_pull_none>,
			<1 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PD0 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PD1 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>,
			<0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>,
			<3 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

&vcc3v3_lcd_n {
	/delete-property/ gpio;
};

&vcc5v0_host {
	/delete-property/ gpio;
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
};

&vcc_mipicsi1 {
	/delete-property/ gpio;
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
};

&vcc_mipidcphy0 {
	/delete-property/ gpio;
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
};

&pcie0 {
	/delete-property/ reset-gpios;
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
	status = "disabled";
};

&hdmi {
	enable-gpios = <&gpio4 RK_PA1 GPIO_ACTIVE_HIGH>;
};

&uart1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart1m0_xfer>;
};

&uart2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart2m1_xfer>;
};

&uart8 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart8m2_xfer>;
};

&uart4 {
	status = "disabled";
};

&ufs {
	status = "disabled";
};

